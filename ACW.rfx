     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D Texture0;
uniform float pixelSize;
uniform vec4 GlowColor;
varying vec2 vTexCoord;
varying vec3 vNormal;



vec4 texSmoothing(sampler2D Texture, vec2 uv)
{
   vec4 smoothedCol=vec4(0.0);
   for (int i=-2; i<=2; i++)
   {
      for (int j=-2; j<=2; j++)
      {
         smoothedCol += texture2D(Texture, uv + vec2(i, j)*pixelSize );
      }
   }
return smoothedCol/25.0;
}
void main(void)
{
   float Intensity = normalize( vNormal ).z * 0.5 + 0.5;
   vec4 teapot= texture2D(Texture0, vTexCoord);
   vec4 teapotSmoothed=texSmoothing(Texture0, vTexCoord);
   if(teapot.r>0.1)
   gl_FragColor=teapot;
   else
   gl_FragColor = teapotSmoothed.rgba * GlowColor * Intensity * 6; 
   
   

}]]></RmShaderCode></RmGLShader>
