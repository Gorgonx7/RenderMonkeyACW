     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D baseMap;
uniform float PartShape;
varying vec2 Texcoord;
varying float Color;
void main( void )
{

   float fade = pow(dot(Texcoord, Texcoord), PartShape);
   
   
   gl_FragColor =  (1.0 - fade) * texture2D( baseMap,vec2(Color, 0.0));
    
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float T;
uniform mat4 viewInv;
uniform float SystemHeight;
uniform float Speed;
uniform float ParticalSpread;
uniform float Shape;
uniform float BillBoardSize;
uniform vec3 Position;

varying vec2 Texcoord;
varying float Color;

void main( void )
{   
    float t = fract(gl_Vertex.z + Speed * T);
    float shape = pow(t, Shape); 
    vec3 Pos;
    Pos.x = ParticalSpread * shape * cos(62.0 * gl_Vertex.z);
    Pos.z = ParticalSpread * shape * sin(163.0 * gl_Vertex.z);
    Pos.y = SystemHeight * t;
    
    
    
    Pos += BillBoardSize * (gl_Vertex.x * viewInv[0] + gl_Vertex.y * viewInv[1]).xyz;
    Pos += Position;
    
    
    
    
    gl_Position = gl_ModelViewProjectionMatrix * vec4(Pos, 1.0);
    //Texcoord = sign(gl_Vertex.xy)*0.5 + 0.5;
    Texcoord = gl_Vertex.xy;
    Color = 1.0 - t;
    
    
}]]></RmShaderCode></RmGLShader>
